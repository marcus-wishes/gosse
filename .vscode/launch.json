{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "GoSSE No Args",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
		},
		{
			"name": "GoSSE Help",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": ["--help"]
		},
		{
			"name": "GoSSE Confilg file",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": [
				"-config=config.toml"
			]
		},
		{
			"name": "GoSSE CMD Line",
			"type": "go",
			"request": "launch",
			"mode": "auto",
			"program": "${workspaceFolder}",
			"args": [
				"-port=6000",
				"-paths=ticket_created,ticket_updated,ticket_deleted",
				"-method=PUT",
			]
		}
	]
}